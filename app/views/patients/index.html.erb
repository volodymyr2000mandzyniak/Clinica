<h1>All Doctors</h1>

<ul>

  <% @patients.each do |patient| %>
    <li>
      <h3><%= link_to patient.full_name, patient_path(patient) %></h3>
      <h3><%= patient.email %></h3>
      <h3><%= patient.phone %></h3>
      <h3><%= patient.city %></h3>
      <h3><%= patient.age %></h3><hr>
    </li>
  <% end %>
</ul>
